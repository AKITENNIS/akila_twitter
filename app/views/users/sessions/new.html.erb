<div class="container" id="mainContent">

  <div class="akitennisclass1305">
    <h2>ログイン</h2>
  </div>


  <div class="akitennisclass1526">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>


      <div class="akitennisclass1526">
        <label class="akitennisclass1063" for="user_email"><span class="label akitennisclass1172">必須</span>
          メールアドレス</label>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "akitennisclass1200", id: "user_email" %>
      </div>

      <div class="akitennisclass1526">
        <label class="akitennisclass1063" for="user_email"><span class="label akitennisclass1172">必須</span>
          パスワード</label>
        <%= f.password_field :password, autofocus: true, autocomplete: "password", class: "akitennisclass1200", id: "user_password" %>
      </div>
      <hr>


      <div class="akitennisclass2348">
        <div class="row">
          <div class="akitennisclass1831 col-md-offset-2 akitennisclass1837">
            <%= link_to "新規会員登録", new_registration_path(resource_name), class: "btn akitennisclass1313 akitennisclass1620" %>
            <br/>
          </div>
          <div class="akitennisclass1831 akitennisclass1837">
            <%= f.submit "ログイン", class: "btn akitennisclass1316 akitennisclass1620" %>
          </div>
        </div>
      </div>
    <% end %>

  </div>
</div>
